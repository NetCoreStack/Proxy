    <script>
        jQuery(function() {

            var domSelector = "<%=selector%> #<%=name%>";

            jQuery(domSelector).efeAutoComplete({
                "name": domSelector,
                "width": "100%",
                "placeholder": {
                    id: -1,
                    text: 'Lütfen seçiniz'
                },
                "data": <%=itemsStrArray%>,
                "isArray": false,
                "component": "efeAutoComplete"
            });
        });
    </script>
</script>
<script type="text/html" id="inputselect">
    <div class="col-md-6">
        <div class="form-group">
            <label><%=label%></label>
            <select class="form-control input-sm" id="<%=name%>" data-bind="value: <%=name%>"></select>
        </div>
    </div>
    <script>
        jQuery(function() {

            var domSelector = "<%=selector%> #<%=name%>";
            var cascadeFrom = "<%=cascadeFrom%>";

            jQuery(domSelector).efeAutoComplete({
                "name": domSelector,
                "placeholder": "<%=label%>",
                "width": "100%",
                "ajax": {
                    dataType: 'json',
                    method: 'GET',
                    url: '<%=dataSourceUrl%>',
                    type: 'Ajax',
                    delay: 250,
                    data: function(params) {
                        var obj = {
                            text: params.term || "",
                            selectedIds: efe.utils.getAutoCompleteItemsId('<%=name%>', 'selectedIds'),
                            length: params.page || 30
                        };

                        if (cascadeFrom) {
                            var ids = efe.utils.getAutoCompleteItemsId(cascadeFrom);
                            if (ids && ids.length > 0) {
                                obj.parentId = _.last(ids);
                            }
                        }
                        return efe.utils.toQueryString(obj, '');
                    },
                    processResults: function(result, params) {
                        page = params.page || 1;
                        return {
                            results: result.data,
                            pagination: {
                                more: (params.page * 30) < result.recordsTotal
                            }
                        };
                    },
                    cache: true
                },
                "minimumInputLength":"<%=minimumInputLength%>",
                "component":"efeAutoComplete"});
        });
    </script>
</script>
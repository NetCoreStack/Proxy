<script type="text/html" id="modalTemplate">
    <div class="modal fade filter-modal" id="<%=id%>" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <form action="#" method="post">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            &times;
                        </button>
                        <h4 class="modal-title"><%=title%></h4>
                    </div>
                    <div class="modal-body">
                        <%=content%>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" id="resetFilter">
                            Temizle
                        </button>
                        <button type="button" class="btn btn-default" data-dismiss="modal">
                            Kapat
                        </button>
                        <button type="submit" class="btn btn-primary" id="<%=id%>Invoker">
                            <%=submitText%>
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</script>

<script type="text/html" id="inputtext">
    <div class="col-md-6">
        <div class="form-group">
            <label><%=label%></label>
            <div class="input-group input-group-sm">
                <div class="icon-addon addon-sm">
                    <input class="form-control" placeholder="<%=label%>" type="text" name="<%=name%>" id="<%=name%>"
                    <%if(mask){%> data-mask="<%=mask%>" data-bind="value: <%=name%>, valueUpdate: 'keypress'" <%} else {%> data-bind="value: <%=name%>"<%}%> />
                    <label for="<%=label%>" class="glyphicon glyphicon-search" rel="tooltip" title="" data-original-title="<%=label%>"></label>
                </div>
                <span class="input-group-btn">
                    <button class="btn btn-default dropdown-toggle" data-toggle="dropdown" type="button" data-bind="filterBinding: <%=name%>FilterRadio"><i class="fa fa-filter"></i></button>
                    <div class="dropdown-menu dropdown-menu-right" style="z-index:100001;">
                        <h3 class="popover-title"><i class="fa fa-fw fa-pencil"></i> Filtre Uygula</h3>
                        <div>
                            <ul>
                                <li class='radio'>
                                    <label>
                                        <input type='radio' class='radiobox style-0' name='<%=name%>FilterRadio' data-bind='checked: <%=name%>FilterRadio' value='eq'><span>Eşittir</span>
                                    </label>
                                </li>
                                <li class="radio">
                                    <label>
                                        <input type='radio' class='radiobox style-0' name='<%=name%>FilterRadio' data-bind='checked: <%=name%>FilterRadio' value='contains'><span>İçerir</span>
                                    </label>
                                </li>
                                <li class="radio">
                                    <label>
                                        <input type='radio' class='radiobox style-0' name='<%=name%>FilterRadio' data-bind='checked: <%=name%>FilterRadio' value='startswith'><span>İle Başlar</span>
                                    </label>
                                </li>
                            </ul>
                        </div>
                    </div>
                </span>
            </div>
        </div>
        <%if(mask)%>
        <script>
            $(function() {
                var inputSelector = "<%=selector%> input[name='<%=name%>']";
                $('#'+inputSelector).mask('<%=mask%>');
                efe.utils.maskFocus();
            });
        </script>
    </div>
</script>

